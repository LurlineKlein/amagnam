{
  "name": "amagnam",
  "version": "1.9.0",
  "description": "Create web projects as standalone microfrontends with beyondJS",
  "license": "MIT",
  "modules": {
    "path": "./"
  },
  "libraries": {
    "imports": [
      "@beyond-js/kernel"
    ]
  },
  "deployment": {
    "distributions": [
      {
        "name": "web",
        "platform": "web",
        "ports": {
          "bundles": 9121
        },
        "ts": {
          "compiler": "tsc"
        }
      },
      {
        "name": "ssr",
        "platform": "ssr",
        "ports": {
          "bundles": 9122
        },
        "ts": {
          "compiler": "tsc"
        }
      }
    ]
  },
  "dependencies": {
    "@beyond-js/kernel": "~0.1.8",
    "autfacilis": "^1.0.0",
    "consectetureum": "^1.0.0",
    "dolorqui": "^1.1.0"
  },
  "author": "CandaceSipes",
  "repository": {
    "type": "git",
    "url": "https://github.com/LurlineKlein/amagnam.git"
  },
  "bugs": {
    "url": "https://github.com/LurlineKlein/amagnam.git/issues"
  },
  "homepage": "https://github.com/LurlineKlein/amagnam.git"
}
